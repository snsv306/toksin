{
  "name": "factchecking-vk",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chrome Extension",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "web_accessible_resources": [
    "styles/styles.css"
  ],
  "permissions": [
    "tabs",
    "background",
    "https://damp-cove-16570.herokuapp.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://vk.com/*",
        "https://vk.com/*"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "css": [
        "styles/styles.css"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}